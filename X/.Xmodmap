! Switch capslock to esc
clear Lock
keycode 9 = Caps_Lock NoSymbol Caps_Lock
keycode 66 = Escape NoSymbol Escape

! Switch ctrl to alt
clear control

remove mod4 = Super_R
add control = Super_R
add mod4 = Control_R

clear shift
keycode 62 = Return
keycode 50 = Shift_L
keycode 36 = Shift_R NoSymbol Shift_R


